# Default environment variables for fastlane
# These will be overridden by CI environment variables

# Non-interactive mode
FASTLANE_OPT_OUT_USAGE=1
FASTLANE_SKIP_UI=1
FASTLANE_HIDE_CHANGELOG=1
FASTLANE_SKIP_UPDATE_CHECK=1

# iOS settings
APPLE_TEAM_ID=YOUR_TEAM_ID
IOS_BUNDLE_ID=com.yourcompany.exporeactnativepaper

# Android settings
ANDROID_PACKAGE_NAME=com.yourcompany.exporeactnativepaper
ANDROID_KEYSTORE_PATH=android/app/release.keystore

# Output directory
FL_OUTPUT_DIR=./build 